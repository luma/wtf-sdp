{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": "./src",
    "includePattern": ".+\\.js$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./doc/gen",
    "template": "node_modules/docdash",
    "readme": "./README.md",
    "package": "./package.json",
    "encoding": "utf8",
    "recurse": true,
    "verbose": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "linenums": true,
    "dateFormat": "MMMM Do YYYY, h:mm:ss a"
  }
}
