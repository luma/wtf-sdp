{
  "name": "wtfsdp",
  "version": "1.0.0-pre.0",
  "engines": {
    "node": ">=5.0.0"
  },
  "main": "lib/index.js",
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "buildParser": "pegjs src/sdp.pegjs src/grammar.peg.js",
    "babelify": "babel src -d lib",
    "build": "npm run buildParser && npm run babelify",
    "todo": "leasot --reporter markdown example/**/*.js src/**/*.js test/**/*.js > TODO.md; exit 0",
    "doc": "jsdoc --configure .jsdocrc",
    "lint": "eslint src/**/*.js",
    "cov": "BABEL_ENV=test rm -rf ./coverage && babel-node ./node_modules/istanbul/lib/cli.js cover --dir ./reports/coverage _mocha -- --opts ./test/mocha.opts ./test",
    "test": "BABEL_ENV=test npm run buildParser && npm run lint && mocha --opts ./test/mocha.opts ./test",
    "test-travis": "BABEL_ENV=test rm -rf ./coverage && npm run buildParser && babel-node node_modules/.bin/istanbul cover --dir ./reports/coverage --report lcovonly _mocha -- --opts ./test/mocha.opts ./test",
    "report-to-codeclimate": "codeclimate-test-reporter < --dir ./reports/coverage/lcov.info",
    "prepublish": "npm run build",
    "example": "babel-node example/test.js",
    "example2": "babel-node example/test2.js"
  },
  "author": {
    "name": "Rolly Fordham",
    "email": "rolly@luma.co.nz"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/luma/wtfsdp.git"
  },
  "dependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.7.4",
    "decko": "^1.1.3",
    "lodash.memoize": "^4.0.3",
    "pegjs-backtrace": "^0.1.1",
    "results": "^0.13.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^4.1.8",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-node5": "^11.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-register": "^6.7.2",
    "bluebird": "^3.3.5",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "codeclimate-test-reporter": "^0.3.1",
    "docdash": "^0.1.0",
    "eslint": "^1.10.3",
    "eslint-config-es6": "luma/eslint-config-es6",
    "eslint-plugin-require-path-exists": "^1.1.5",
    "istanbul": "^1.0.0-alpha.2",
    "leasot": "^3.2.2",
    "mocha": "^2.4.5",
    "mocha-clean": "^1.0.0",
    "pegjs": "^0.9.0",
    "prettyjson": "^1.1.3",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0"
  }
}
